import groovy.sql.GroovyRowResult
import com.eviware.soapui.support.GroovyUtilsPro

// Check whether TestCase is not running under Test Load.
if (context.LoadTestRunner == null)
{
	log.debug "Checking the database content."
	
	String stat_name = context.expand( '${#TestCase#stat_name}' )
	String stat_value = context.expand( '${#TestCase#stat_value}' )
	def res_height = context.expand( '${DataGen#res_height}' )
	def res_width = context.expand( '${DataGen#res_width}' )
	
	GroovyUtilsPro utils  = new GroovyUtilsPro( context )
	context.sql = utils.getGroovySql("Local db")
		
	String sqlQuery = "select count(*) from resolution as res, statistic as stat where res.id = stat.resolution_id and stat.name = '$stat_name' and stat.value = '$stat_value' AND res.height = $res_height AND res.width = $res_width"
	GroovyRowResult result = context.sql.firstRow(sqlQuery)
	Integer count = result.getAt(0)
	context.sql.close()
	
	if (count == null || count == 0) {
		testRunner.fail("No rows in database, the count is equal: '" + count + "'.")
	}
	else if (count > 1) {
	   testRunner.fail("More than one expected row in database, the count is equal: '" + count + "'.")
	}
	
	log.info "Database checked. OK"
}</script></con:config></con:testStep><con:loadTest id="2670b31e-5bc0-4c6f-91eb-8eab53a0a323" name="LoadTest 1"><con:settings><con:setting id="HttpSettings@close-connections">false</con:setting></con:settings><con:threadCount>5</con:threadCount><con:startDelay>0</con:startDelay><con:sampleInterval>250</con:sampleInterval><con:calculateTPSOnTimePassed>true</con:calculateTPSOnTimePassed><con:resetStatisticsOnThreadCountChange>true</con:resetStatisticsOnThreadCountChange><con:historyLimit>-1</con:historyLimit><con:testLimit>10</con:testLimit><con:limitType>TIME</con:limitType><con:loadStrategy><con:type>Simple</con:type></con:loadStrategy><con:assertion type="Step Status" name="Step Status"/><con:maxAssertionErrors>100</con:maxAssertionErrors><con:cancelExcessiveThreads>true</con:cancelExcessiveThreads><con:strategyInterval>500</con:strategyInterval><con:reportParameters/></con:loadTest><con:setupScript>import com.eviware.soapui.support.GroovyUtilsPro
